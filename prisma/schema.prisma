generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model ACESAQUIS {
  CD_ACESAQUIS Int        @id @default(0)
  CD_AQUISICAO Int?       @default(0)
  CD_ACESSORIO Int?       @default(0) @db.SmallInt
  ACESSORIO    ACESSORIO? @relation(fields: [CD_ACESSORIO], references: [CD_ACESSORIO], onDelete: NoAction, onUpdate: NoAction, map: "ACESSORIOACESAQUIS")
  AQUISICAO    AQUISICAO? @relation(fields: [CD_AQUISICAO], references: [CD_AQUISICAO], onDelete: Cascade, map: "AQUISICAOACESAQUIS")

  @@index([CD_ACESSORIO], map: "ACESSORIOACESAQUIS")
  @@index([CD_AQUISICAO], map: "AQUISICAOACESAQUIS")
}

model ACESSO {
  CD_ACESSO  Int       @id @default(autoincrement())
  DT_ACESSO  DateTime? @db.DateTime(0)
  CD_USUARIO Int?
  NM_USUARIO String?   @db.VarChar(50)
  TP_ACESSO  Int?      @default(0) @db.SmallInt

  @@index([CD_USUARIO], map: "ik_acesso_CDUSUARIO")
}

model ACESSORIO {
  CD_ACESSORIO Int         @id @default(0) @db.SmallInt
  NM_ACESSORIO String      @db.VarChar(30)
  ACESAQUIS    ACESAQUIS[]
}

model AGENCIA {
